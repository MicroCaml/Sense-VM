data Maybe a where
  Just   : a -> Maybe a
  Nothng : Maybe a

data List a where
  Nil  : List a
  Cons : a -> List a -> List a

head : List a -> Maybe a
head (Cons a b) = Just a
head Nil        = Nothing

take : Int -> List a -> List a
take 0 _ = Nil
take n Nil = Nil
take n (Cons h t) = Cons h (take (n - 1) t)

drop : Int -> List a -> List a
drop 0 r = r
drop n Nil = Nil
drop n (Cons a r) = drop (n-1) r

length : List a -> Int
length Nil = 0
length (Cons a b) = 1 + length b